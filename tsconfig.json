{
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "module": "ES2022",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}